 { id } = useParams();\n  return (\n    <div className='header bg-yellow-100 w-1/2 mx-auto border-2 m-16 rounded-lg'>\n      <ProjectDisplayHeader />\n      <ProjectDisplayBody />\n    </div>\n  )\n}\n\nexport default ProjectDisplay;\n","/home/clarion/app_academy/soloproject/authenticate-me/frontend/src/components/ProjectDisplayHeader/index.js",[],"/home/clarion/app_academy/soloproject/authenticate-me/frontend/src/components/ProjectDisplayBody/index.js",["177","178","179"],"import React, {useEffect} from 'react'\nimport BigImageProject from './BigImageProject';\nimport ThumbnailImages from './ThumbnailImages';\nimport Discussion from '../Discussion';\nimport summary from './summary';\nimport {useImageContext, useimageContext} from '../../context/imagecontext';\n\nconst ProjectDisplayBody = () => {\n  const {imageArr, mainImage, setMainImage} = useImageContext();\n  let firstImage = imageArr[0].webformatURL;\n\n  useEffect(() => {\n    setMainImage(firstImage);\n  },[])\n\n  return (\n    <div className='mainprojectcontainer'>\n      <div className='leftside p-4'>\n        <section className='bg-white flex flex-col p-4 rounded-lg'>\n          <BigImageProject />\n          <ThumbnailImages />\n          <div className='descripitiontext p-4'>\n            {summary}\n          </div>\n        </section>\n      </div>\n      <Discussion />\n    </div>\n  )\n}\n\nexport default ProjectDisplayBody;\n","/home/clarion/app_academy/soloproject/authenticate-me/frontend/src/components/ProjectDisplayBody/BigImageProject/index.js",["180"],"import React from 'react';\nimport {useImageContext} from '../../../context/imagecontext';\nlet defaultImage = \"https://mycontent-space.s3.amazonaws.com/everything+is+going+to+be+alright.jpg\";\nconst BigImageProject = () => {\n  const { mainImage } = useImageContext();\n  return (\n    <div>\n      <div className=''>\n         <img className='mx-auto w-auto h-auto rounded-lg'src={mainImage} alt=\"flower\" />\n      </div>\n    </div>\n  )\n}\n\nexport default BigImageProject;\n","/home/clarion/app_academy/soloproject/authenticate-me/frontend/src/components/ProjectDisplayBody/ThumbnailImages/index.js",["181"],"import React from 'react'\nimport {useImageContext} from '../../../context/imagecontext';\n\n\n\nconst ThumbnailImages = () => {\n  const { imageArr, setMainImage } = useImageContext();\n\n  const imageDivCreate = (num, array) => {\n    const newArray = [];\n    let newThumbnail;\n    let url;\n    for (let i = 0; i < num; i++) {\n      url = array[i].webformatURL;\n      newThumbnail = <div className='bg-green-500 h-16 w-16 m-2 cursor-pointer'>\n        <a onClick={e => setMainImage(e.target.src)}><img className='h-full' src={url} alt=\"\" /></a>\n      </div>\n      newArray.push(newThumbnail);\n    }\n    return newArray;\n  }\n\n let displayDivs = imageDivCreate(6, imageArr);\n\n  return (\n    <div className='flex border-b-2'>\n      {displayDivs.map(ele => ele)}\n    </div>\n  )\n}\n\nexport default ThumbnailImages;\n","/home/clarion/app_academy/soloproject/authenticate-me/frontend/src/components/Discussion/index.js",["182"],"import React, {useEffect} from 'react'\nimport DiscussionMain from './DiscussionMain';\nimport DiscussionInput from './DiscussionInput';\nimport {useDispatch} from 'react-redux';\nimport { useParams } from 'react-router-dom';\nimport * as discussionActions from '../../store/discussion';\n\nconst Discussion = () => {\n  const {id} = useParams();\n  const dispatch = useDispatch();\n\n  useEffect(() => {\n    dispatch(discussionActions.getDiscussions(id));\n  }, [dispatch])\n\n  return (\n    <div>\n      <h2 className='font-bold'>Discussion</h2>\n      <div className='discussions bg-white p-6 m-4 rounded-lg'>\n        <DiscussionInput />\n        <DiscussionMain />\n      </div>\n    </div>\n  )\n}\n\nexport default Discussion;\n","/home/clarion/app_academy/soloproject/authenticate-me/frontend/src/context/imagecontext.js",["183"],"import { createContext, useState, useConte