      <li>{image.tags}</li>\n              <li>\n                <CommentCount />\n              </li>\n            </ul>\n          </div>\n          </div>\n          </NavLink>\n        })}\n      </div>\n    </>\n  )\n}\n\nexport default ProjectHome;\n","/home/clarion/app_academy/soloproject/authenticate-me/frontend/src/components/ProjectHome/CommentCount.js",[],"/home/clarion/app_academy/soloproject/authenticate-me/frontend/src/components/ProjectDisplay/index.js",["187"],"import React from 'react'\nimport {useParams} from 'react-router-dom';\nimport ProjectDisplayHeader from '../ProjectDisplayHeader';\nimport ProjectDisplayBody from '../ProjectDisplayBody';\n\nconst ProjectDisplay = () => {\n  const { id } = useParams();\n  return (\n    <div className='header bg-yellow-100 w-1/2 mx-auto border-2 m-16 rounded-lg'>\n      <ProjectDisplayHeader />\n      <ProjectDisplayBody />\n    </div>\n  )\n}\n\nexport default ProjectDisplay;\n","/home/clarion/app_academy/soloproject/authenticate-me/frontend/src/components/ProjectDisplayHeader/index.js",[],"/home/clarion/app_academy/soloproject/authenticate-me/frontend/src/components/ProjectDisplayBody/index.js",["188","189","190"],"import React, {useEffect} from 'react'\nimport BigImageProject from './BigImageProject';\nimport ThumbnailImages from './ThumbnailImages';\nimport Discussion from '../Discussion';\nimport summary from './summary';\nimport {useImageContext, useimageContext} from '../../context/imagecontext';\n\nconst ProjectDisplayBody = () => {\n  const {imageArr, mainImage, setMainImage} = useImageContext();\n  let firstImage = imageArr[0].webformatURL;\n\n  useEffect(() => {\n    setMainImage(firstImage);\n  },[])\n\n  return (\n    <div className='mainprojectcontainer'>\n      <div className='leftside p-4'>\n        <section className='bg-white flex flex-col p-4 rounded-lg'>\n          <BigImageProject />\n          <ThumbnailImages />\n          <div className='descripitiontext p-4'>\n            {summary}\n          </div>\n        </section>\n      </div>\n      <Discussion />\n    </div>\n  )\n}\n\nexport default ProjectDisplayBody;\n","/home/clarion/app_academy/soloproject/authenticate-me/frontend/src/components/ProjectDisplayBody/BigImageProject/index.js",["191"],"import React from 'react';\nimport {useImageContext} from '../../../context/imagecontext';\nlet defaultImage = \"https://mycontent-space.s3.amazonaws.com/everything+is+going+to+be+alright.jpg\";\nconst BigImageProject = () => {\n  const { mainImage } = useImageContext();\n  return (\n    <div>\n      <div className=''>\n         <img className='mx-auto w-auto h-auto rounded-lg'src={mainImage} alt=\"flower\" />\n      </div>\n    </div>\n  )\n}\n\nexport default BigImageProject;\n","/home/clarion/app_academy/soloproject/authenticate-me/frontend/src/components/ProjectDisplayBody/ThumbnailImages/index.js",["192"],"import React from 'react'\nimport {useImageContext} from '../../../context/imagecontext';\n\n\n\nconst ThumbnailImages = () => {\n  const { imageArr, setMainImage } = useImageContext();\n\n  const imageDivCreate = (num, array) => {\n    const newArray = [];\n    let newThumbnail;\n    let url;\n    for (let i = 0; i < num; i++) {\n      url = array[i].webformatURL;\n      newThumbnail = <div className='bg-green-500 h-16 w-16 m-2 cursor-pointer'>\n        <a onClick={e => setMainImage(e.target.src)}><img className='h-full' src={url} alt=\"\" /></a>\n      </div>\n      newArray.push(newThumbnail);\n    }\n    return newArray;\n  }\n\n let displayDivs = imageDivCreate(6, imageArr);\n\n  return (\n    <div className='flex border-b-2'>\n      {displayDivs.map(ele => ele)}\n    </div>\n  )\n}\n\nexport default ThumbnailImages;\n","/home/clarion/app_academy/soloproject/authenticate-me/frontend/src/components/Discussion/index.js",["193"],"import React, {useEffect} from 'react'\nimport DiscussionMain from './DiscussionMain';\nimport DiscussionInput from './DiscussionInput';\nimport {useDispatch} from 'react-redux';\nimport { useParams } from 'react-router-dom';\nimport * as discussionActions from '../../store/discussion';\n\nconst Discussion = () => {\n  const {id} = useParams();\n  const dispatch = useDispatch();\n\n  useEffect(() => {\n    dispatch(discussionActions.getDiscussions(id));\n  }, [dispatch])\n\n  return (\n    <div>\n      <h2 className='font-bold'>Discussion</h2>\n      <div className='discussions bg-white p-6 m-4 rounded-lg'>\n        <DiscussionInput />\n        <DiscussionMain />\n      </div>\n    </div>\n  )\n}\n\nexport default Discussion;\n","/home/clarion/app_academy/soloproject/authenticate-me/frontend/src/context/imagecontext.js",["194"],"import { createContext, useState, useContext, useEffect } from 'react';\nexport const ImageContext = createContext();\nlet defaultImage = <img src=\"https://mycontent-space.s3.amazonaws.com/everything+is+going+to+be+alright.jpg\" alt=\"a\" />\n\nexport function useImageContext() {\n  return useContext(ImageContext);\n}\n\nexport function ImageProvider(props) {\n  const [imageArr, setImageArr] = useState([]);\n  const [mainImage, setMainImage] = useState();\n  useEffect(() => {\n    fetch(`https://pixabay.com/api/?key=20010415-a6682cfb4ce63170711548b9b&q=yellow+flowers&image_type=photo`)\n    .then(res => res.json())\n    .then(data => setImageArr(data.hits))\n    .then(data => setMainImage(data[0].webformatURL))\n    .catch(e => console.log(e));\n  }, []);\n  return (\n    <ImageContext.Provider value={{imageArr, setImageArr, mainImage, setMainImage}}>\n      {props.children}\n    </ImageContext.Provider>\n  )\n}\n","/home/clarion/app_academy/soloproject/authenticate-me/frontend/src/components/ProjectDisplayBody/summary.js",[],"/home/clarion/app_academy/soloproject/authenticate-me/frontend/src/components/Discussion/UserDisplay/index.js",["195"],"import React, {useEffect, useState} from 'react'\nimport {useSelector} from 'react-redux';\n\nconst UserDisplay = ({comment}) => {\n  const [myUser, setMyUser] = useState('');\n  let foundUser;\n  const users = useSelector(state => Object.values(state.user));\n  useEffect(() => {\n    foundUser = users.filter(user => user.id === comment.userId)\n    setMyUser(foundUser[0])\n    console.log(myUser)\n  },[comment, myUser]);\n\n  return (\n    <>\n    {myUser && <>\n      <div className='mainuser flex items-start'>\n      <div className='userdetails inline-flex flex-row flex-nowrap'>\n        <div className='userimage'>\n          <div className='h-8 w-8 rounded-full bg-blue-800 mr-6'>...</div>\n        </div>\n      <div className='usercontent flex flex-col'>\n        <span>{myUser.name}</span>\n        <span>{myUser.username}</span>\n      </div>\n      </div>\n    </div>\n    </>}\n  </>\n  )\n}\n\nexport default UserDisplay;\n","/home/clarion/app_academy/soloproject/authenticate-me/frontend/src/components/Discussion/Comment/index.js",["196"],"import React, {useState, useEffect} from 'react'\nimport {useDispatch} from 'react-redux';\nimport * as discussionActions from '../../../store/discussion';\nimport {useParams} from 'react-router-dom';\nimport Reply from '../Reply';\nimport UserDisplayReply from '../UserDisplayReply';\nimport ReplyField from '../ReplyField';\n\nconst Comment = ({comment}) => {\n  const [replyToggle, setReplyToggle] = useState(false);\n  const {id} = useParams();\n  const dispatch = useDispatch();\n  const deleteButton = () => {\n    dispatch(discussionActions.deleteOneDiscussion(id, comment.id))\n  }\n  console.log('comments are here:', comment.Reviews)\n\n  const toggleChange = () => setReplyToggle(value => !value);\n\n  return (\n    <>\n      <div className='commentcontainer p-6 ml-8 my-2 border-l-4'>\n        <div className='commentmain'>\n          <div className='comment'>\n            {comment.content}\n          </div>\n        </div>\n        <button onClick={toggleChange}className='bg-blue-200 rounded-lg p-1'>Reply</button>\n        <button onClick={deleteButton} className='bg-red-200 rounded-lg 